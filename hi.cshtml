@{
 var db = Database.Open("SmallBakery"); 
 var selectQueryString = "SELECT * FROM Contacts ORDER BY Name"; 
 }
 <html> 
 <body> 
 <h1>Small Bakery Products</h1> 
 <table border="1" > 
 <tr>
 <th>Id</th> 
 <th>Product</th> 
 <th>Description</th> 
 <th>Price</th> 
 </tr>
@foreach(var row in db.Query(selectQueryString))
 {
 <tr> 
 <td>@row.ID</td> 
 <td>@row.Name</td> 
 <td>@row.Tel</td> 
 <td align="right">@row.Tel</td> 
 </tr> 
}
 </table> 


